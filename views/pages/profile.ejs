<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
</head>

<body>
    <header>
        <%- include('../partials/header') %>
    </header>

    <main>
        <h2>Profile</h2>
        <h3>Your Owned Books</h3>
        <div class="flex-center">
            <% for(var i=0; i < userbooklist.length; i++) { %>
            <a id="<%= userbooklist[i].book.id._ %>" href="/book">
                <img class="book-result" src="<%= userbooklist[i].book.image_url %>">
            <% } %>
            </a>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer') %>
    </footer>

</body>
</html>
